{"ast":null,"code":"import { call, fork, put, takeEvery } from \"redux-saga/effects\";\nimport * as actions from '../actions/users';\nimport { useGetUsers } from \"../../../actions\";\nimport * as api from '../api/users'; // Step 9: Create getUser function which watcher will call\n\nfunction* getUsers() {\n  try {\n    const result = yield call(api.getUsers());\n    console.log(result.data);\n    yield put(actions.getUsersSuccess({\n      items: result.data\n    }));\n  } catch (e) {\n    console.log(e);\n  }\n} // Step 8: Create User Saga Watcher\n// Whenever get-user action dispatch this saga will watch and call getUsers function\n\n\nfunction* watchGetUsersRequest() {\n  yield takeEvery(actions.Types.GET_USERS_REQUESTS, getUsers);\n} // Step 10: Create fork\n\n\nconst usersSaga = [fork(watchGetUsersRequest)];\nexport default usersSaga;","map":{"version":3,"sources":["/home/ubuntu/work/github/Starter Project/starter-app-project/components/redux/sagas/users.js"],"names":["call","fork","put","takeEvery","actions","useGetUsers","api","getUsers","result","console","log","data","getUsersSuccess","items","e","watchGetUsersRequest","Types","GET_USERS_REQUESTS","usersSaga"],"mappings":"AAAA,SAAQA,IAAR,EAAcC,IAAd,EAAoBC,GAApB,EAAyBC,SAAzB,QAAyC,oBAAzC;AACA,OAAO,KAAKC,OAAZ,MAAyB,kBAAzB;AACA,SAAQC,WAAR,QAA0B,kBAA1B;AACA,OAAO,KAAKC,GAAZ,MAAqB,cAArB,C,CAEA;;AACA,UAAUC,QAAV,GAAqB;AACjB,MAAI;AACA,UAAMC,MAAM,GAAG,MAAMR,IAAI,CAACM,GAAG,CAACC,QAAJ,EAAD,CAAzB;AACAE,IAAAA,OAAO,CAACC,GAAR,CAAYF,MAAM,CAACG,IAAnB;AACA,UAAMT,GAAG,CAACE,OAAO,CAACQ,eAAR,CAAwB;AAC9BC,MAAAA,KAAK,EAAEL,MAAM,CAACG;AADgB,KAAxB,CAAD,CAAT;AAGH,GAND,CAME,OAAOG,CAAP,EAAU;AACRL,IAAAA,OAAO,CAACC,GAAR,CAAYI,CAAZ;AACH;AACJ,C,CAED;AACA;;;AACA,UAAUC,oBAAV,GAAiC;AAC7B,QAAMZ,SAAS,CAACC,OAAO,CAACY,KAAR,CAAcC,kBAAf,EAAmCV,QAAnC,CAAf;AACH,C,CAED;;;AACA,MAAMW,SAAS,GAAG,CACdjB,IAAI,CAACc,oBAAD,CADU,CAAlB;AAIA,eAAeG,SAAf","sourcesContent":["import {call, fork, put, takeEvery} from \"redux-saga/effects\";\nimport * as actions from '../actions/users'\nimport {useGetUsers} from \"../../../actions\";\nimport * as api from '../api/users'\n\n// Step 9: Create getUser function which watcher will call\nfunction* getUsers() {\n    try {\n        const result = yield call(api.getUsers());\n        console.log(result.data);\n        yield put(actions.getUsersSuccess({\n            items: result.data\n        }))\n    } catch (e) {\n        console.log(e)\n    }\n}\n\n// Step 8: Create User Saga Watcher\n// Whenever get-user action dispatch this saga will watch and call getUsers function\nfunction* watchGetUsersRequest() {\n    yield takeEvery(actions.Types.GET_USERS_REQUESTS, getUsers);\n}\n\n// Step 10: Create fork\nconst usersSaga = [\n    fork(watchGetUsersRequest)\n];\n\nexport default usersSaga;\n"]},"metadata":{},"sourceType":"module"}